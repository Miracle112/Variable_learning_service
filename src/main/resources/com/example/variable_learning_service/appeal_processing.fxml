         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Text>
      <TextField fx:id="kind_of_help" layoutX="99.0" layoutY="74.0" promptText="Вид помощи" />
      <TextField fx:id="date" layoutX="99.0" layoutY="155.0" promptText="Дата" />
      <TextField fx:id="time" layoutX="99.0" layoutY="233.0" promptText="Время" />
      <TextField fx:id="comment" alignment="TOP_LEFT" layoutX="353.0" layoutY="149.0" prefHeight="109.0" prefWidth="149.0" promptText="Комментарий" />
      <ComboBox fx:id="specialist" layoutX="352.0" layoutY="74.0" prefWidth="150.0" promptText="Специалист" />
