<AnchorPane prefHeight="375.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.variable_learning_service.AppealProcessingController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../image/background_client.jpg" />
         </image>
      </ImageView>
      <Button fx:id="backBtn" layoutX="528.0" layoutY="330.0" mnemonicParsing="false" text="Назад" />
